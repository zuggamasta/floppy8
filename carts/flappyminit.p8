-- pipe logic
function i_pipes()
	pipes={
		x1=game.maxx,
		y1=rnd((game.maxy-game.miny)/2)+game.miny+(game.maxy-game.miny)/2,
		p1=false
	}
end

function u_pipes()
	pipes.x1 -= game.speed
	if pipes.x1 < game.minx-16 then
		pipes.x1 = game.maxx
		pipes.y1 = rnd(32)+48
		pipes.p1 = false
	end
end

function d_pipes()

	if (bird.pnts >= 1) then
		-- draw bottm tip
		spr(56, pipes.x1, pipes.y1, 2, 1)
		-- draw buttom stump
		for i=0,5 do
			id = 34+flr(i%3)*2
			spr(id, pipes.x1, pipes.y1+8+i*16, 2, 2)
		end

		-- draw top
		for i=0,3 do
			id2 = 42+flr(i%4)*2
			spr(id2, pipes.x1, pipes.y1-48-i*16, 2, 2)
		end
	else
	  id = 150
		spr(134, pipes.x1+6, pipes.y1)
		-- draw buttom helper

		for i=0,16 do
			spr(id, pipes.x1+6, pipes.y1+8+i*8)
		end


		-- draw top
		spr(134, pipes.x1+6, pipes.y1-48+8,1,1,false,true)

		for i=0,16 do
			spr(id, pipes.x1+6, pipes.y1-48-i*8,1,1,false,true)
		end

	end
end


