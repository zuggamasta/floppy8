-- bird logic
function i_bird()
		bird={sp=1,
				x=64,
				y=64,
				vx=0,
				vy=0,
				n=0,
				pnts=0,
				dead=0

				}
	bird.x=game.minx+(game.maxx-game.minx)/3
	bird.y=game.miny+(game.maxy-game.miny)/2
end

function u_bird()
  bird.n -= 1
	if bird.n <0 then bird.n = 0 end

	bird.y += bird.vy

	if bird.dead == 1 then
		bird.dead = 0
		_init()
	end

	if bird.dead == 0 then
		--if (btnp(0)) then bird.x = bird.x-1 end
		--if (btnp(1)) then bird.x = bird.x+1 end
		if (btnp(2) or btnp(4) or btnp(5)) then
			if game.tutorial == 3 then return end
			bird.y = bird.y-1
			bird.vy = -2.2356
			sfx(1)
			bird.n = 16
		else
			bird.vy += game.g
			
		end

		if bird.x > pipes.x1 and not pipes.p1 then
			pipes.p1 = true

		if (bird.y < pipes.y1) and (bird.y > pipes.y1-32) then
			bird.pnts += 1
			game.speed += 0.1
			if game.tutorial >=0 then game.tutorial -=1 end
			sfx(4)
			--game.minx -= 2
			--game.maxx += 2
			--game.miny += 1
			--game.maxy -= 1


		else
			bird.dead = timeout
			sfx(2)
			game.bck = 8
		end
	end

	else
		bird.dead -= 1
		bird.vy += game.g
		bird.n = 0
	end

	-- clamping
	if bird.dead > 0 then
		if bird.y > game.maxy then
			bird.y = game.maxy
			bird.y = bird.y-1
			bird.vy = -2
			sfx(3)
		end
	else
		if bird.y > 128 then
			bird.pnts = 0
			bird.dead = timeout
			sfx(2)
			game.bck = 8
		end
	end
 	
	
	if bird.x < game.minx then bird.x = game.minx end
	if bird.x > game.maxx then bird.x = game.maxx end
 	if bird.y < 0 then bird.y = 0 end

end

function d_bird()
	x= bird.x
	y= bird.y
	size = 2
	if bird.dead > 0 then
		spr(10, x-8, y-8, size, size)
	else
		spr(flr(bird.n/4)%4*2, x-8, y-8, size, size)
	end
end

function u_tutorial()
	if game.tutorial <= 1 then 
		game.tutorial = 0
		return
	end

	if (game.tutorial == 4) or (game.tutorial == 2) then
		if (btnp(2) or btnp(4) or btnp(5)) then
			bird.y = bird.y-1
			bird.vy = -2.2356
			sfx(5)
			bird.n = 16
			game.tutorial -= 1
			game.g = g
		end
	end

	if game.tutorial == 3 then
		if bird.y > game.maxy - 16 then
			game.tutorial = 2
			game.g = 0
			bird.vy=0
		end
	end

	
end

function d_tutorial(x,y)
	if (game.tutorial == 4) or (game.tutorial == 2) then
		spr(128, x-16-4, y+(flr(c/8)%2)-4, 2, 2)
		print_c("OR", x,y+1, 7)
		spr(130, x+4, y+(flr(c/8)%2)-4, 2, 2)

		-- draw arrow
		for i=0, 2 do
			spr(135,bird.x+i*8-12,bird.y-(flr(c/8)%2)+9, 2, 2)
		end

	end

end
