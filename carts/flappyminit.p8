-- extras

function d_points(points)
	x = game.minx+(game.maxx-game.minx)/2
	y = game.miny+8
	print_c(tostr(points), x, y, 7)	
end

function d_front()
	rectfill(0,0,game.minx,128,0) --left
	rectfill(game.maxx,0,128,128,0) --right
	rectfill(0,0,128,game.miny,0) --top
	rectfill(0,game.maxy,128,128,0) --bottom
	rect(game.minx-1,game.miny-1,game.maxx,game.maxy+1,7)
	--debug
	--print(bird.dead,10)
end

function u_clouds()
	c+=0.5
	if c > 128 then c = 0 end
end

function d_clouds(mult)
  newc = c*mult
	map(0,0,-newc)
	map(0,0,-newc+128)
	map(0,0,-newc+256)
	map(0,0,-newc+128+256)
end

function print_c(text,x,y,col)
  black = 0
	print(text,x-#text*2-1,y,black)
	print(text,x-#text*2,y-1,black)
	print(text,x-#text*2+1,y,black)
	print(text,x-#text*2,y+1,black)
	print(text,x-#text*2,y,col)
end

